{
  "name": "phentrieve-frontend",
  "version": "0.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css,scss,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,css,scss,html}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "i18n:check": "node scripts/validate-i18n.js && vue-i18n-extract --vueFiles './src/**/*.{vue,js,ts}' --languageFiles './src/locales/**/*.json'",
    "i18n:report": "vue-i18n-extract --output json --vueFiles './src/**/*.{vue,js,ts}' --languageFiles './src/locales/**/*.json' > i18n-report.json",
    "audit": "npm audit --audit-level=high",
    "audit:prod": "npm audit --omit=dev --audit-level=critical",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@berntpopp/phenopackets-js": "^1.0.2",
    "axios": "^1.13.2",
    "google-protobuf": "^4.0.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuetify": "^3.11.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint-plugin-security": "^3.0.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@mdi/font": "^7.3.67",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.9",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.44.1",
    "vite": "^6.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.9",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n-extract": "^2.0.7"
  }
}
